@model IEnumerable<CRM.Identity.Models.Personnel_Data>
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860

@{
    //ViewData["Title"] = "Список смартфонов";
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Магазин смартфонов</title>
</head>
<body>
    <h3>Смартфоны</h3>
    <table>
        <tr>
            <td>Модель</td>
            <td>Производитель</td>
            <td>Цена</td>
            <td></td>
        </tr>
        @foreach (var personnel_ in Model)
        {
            <tr>
                <td>@personnel_.Name</td>
                <td>@personnel_.Last_Name</td>
             
                <td><a href="~/Home/Buy/@personnel_.ID_Personnal">Купить</a></td>
            </tr>
        }
    </table>
</body>
</html>